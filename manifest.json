{
  "manifest_version": 3,
  "name": "Hate Speech Censoring",
  "version": "1.0.0",
  "action": { "default_popup": "index.html" },
  "permissions": ["tabs", "webNavigation"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/watch?*"],
      "js": ["contentScript.js"]
    }
  ]
}
